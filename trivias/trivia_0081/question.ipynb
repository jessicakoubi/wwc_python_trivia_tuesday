   ],
   "source": [
    "def factorial(n):\n",
    "    r = 1\n",
    "    for i in range(1, n+1):\n",
    "        r *= i\n",
    "\n",
    "    return r\n",
    "\n",
    "print(factorial(4))"
   ]
  },
